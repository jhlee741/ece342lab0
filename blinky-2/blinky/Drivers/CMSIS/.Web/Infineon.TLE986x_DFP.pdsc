  <conditions>
    <!-- conditions are dependency rules that can apply to a component or an individual file -->
    <condition id="cond_any_TLE986x_device">
      <description>Infineon TLE986x device series</description>
      <accept Dvendor="Infineon:7" Dname="TLE9861QXA20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9867QXA20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9867QXA40"/>
      <accept Dvendor="Infineon:7" Dname="TLE9867QXW20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9869QXA20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9862QXA40"/>
    </condition>
    
    <condition id="cond_TLE986x_ConfigWizard_1">
      <!-- Condition for ConfigWizard_v1.x -->
      <description>If ConfigWizard_v1.x is selected, version 2.x must not be selected</description>
      <deny Cclass="Device" Cgroup="ConfigWizard_v2" Csub=""/>
      <require condition="cond_any_TLE986x_device"/>
      <deny Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
      <deny Dvendor="Infineon:7" Dname="TLE9862QXA40"/>
    </condition>
    
    <condition id="cond_TLE986x_ConfigWizard_2">
      <!-- Condition for ConfigWizard_v2.x -->
      <description>If ConfigWizard_v2.x is selected, version 1.x must not be selected</description>
      <deny Cclass="Device" Cgroup="ConfigWizard" Csub=""/>
      <require condition="cond_any_TLE986x_device"/>
    </condition>
    
    <condition id="cond_TLE986x_BF_Step">
      <description>Infineon TLE987x device series, BF-Step</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <accept Dvendor="Infineon:7" Dname="TLE9861QXA20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9867QXA20"/>
      <accept Dvendor="Infineon:7" Dname="TLE9867QXA40"/>
      <accept Dvendor="Infineon:7" Dname="TLE9869QXA20"/>
    </condition>
    
    <!-- component conditions -->
    <condition id="cond_TLE986x_ADC1">
      <accept condition="cond_any_TLE986x_device"/>
      <!-- ADC1 requires ADC2 -->
      <require Cclass="Device" Cgroup="SDK" Csub="ADC2"/>
    </condition>
    <condition id="cond_TLE986x_ADC2">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_BDRV">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_BOOTROM">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_CCU6">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_CSA">
      <accept condition="cond_any_TLE986x_device"/>
      <!-- TLE9868QXB20 has no CSA -->
      <deny Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
      <!-- CSA requires ADC1 -->
      <require Cclass="Device" Cgroup="SDK" Csub="ADC1"/>
    </condition>
    <condition id="cond_TLE986x_DMA">
      <accept condition="cond_any_TLE986x_device"/>
      <!-- TLE9868QXB20 has no DMA -->
      <deny Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
    </condition>
    <condition id="cond_TLE986x_GPT12E">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_INT">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_ISR">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_LIN">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_MON">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_PMU">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_PORT">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_SCU">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_SDADC">
      <accept Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
    </condition>
    <condition id="cond_TLE986x_SSC">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_TIMER2x">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_TIMER3">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_WDT1">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
    <condition id="cond_TLE986x_UART">
      <accept condition="cond_any_TLE986x_device"/>
    </condition>
                                 
    <condition id="cond_TLE986x_startup">                                     
      <!-- conditions selecting Devices -->
      <description>Infineon TLE986x device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Cclass="Device" Cgroup="SDK" Csub="SCU"/>
      <require Cclass="Device" Cgroup="SDK" Csub="BOOTROM"/>
      <require Cclass="Device" Cgroup="SDK" Csub="WDT1"/>
      <require Cclass="Device" Cgroup="SDK" Csub="ISR"/>
      <require Cclass="Device" Cgroup="SDK" Csub="INT"/>
      <require Cclass="Device" Cgroup="SDK" Csub="LIN"/>
      <require Cclass="Device" Cgroup="SDK" Csub="PORT"/>
      <require Cclass="Device" Cgroup="SDK" Csub="PMU"/>
      <accept condition="cond_any_TLE986x_device"/>
      <require Tcompiler="ARMCC"/>
      <accept condition="cond_TLE986x_ConfigWizard_1"/>
      <accept condition="cond_TLE986x_ConfigWizard_2"/>
    </condition>

    <!-- SDADC condition: yes/no -->
    <condition id="cond_TLE986x_noSDADC">
      <description>Infineon TLE986x device series, no SDADC</description>
        <require condition="cond_any_TLE986x_device"/>
        <accept Dvendor="Infineon:7" Dname="TLE986?Q*"/>
        <deny Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
    </condition>
    <condition id="cond_TLE986x_withSDADC">
      <description>Infineon TLE986x device series, with SDADC</description>
        <require condition="cond_any_TLE986x_device"/>
        <accept Dvendor="Infineon:7" Dname="TLE986?-2*"/>
        <accept Dvendor="Infineon:7" Dname="TLE986?_2*"/>
        <accept Dvendor="Infineon:7" Dname="TLE9868QXB20"/>
    </condition>
    <!-- fSYS condition: 40/24MHz -->
    <condition id="cond_TLE986x_40MHz">
      <description>Infineon TLE986x device series, fSYS 40MHz</description>
        <require condition="cond_any_TLE986x_device"/>
        <accept Dvendor="Infineon:7" Dname="TLE986*40"/>
    </condition>
    <condition id="cond_TLE986x_24MHz">
      <description>Infineon TLE986x device series, fSYS 24MHz</description>
        <require condition="cond_any_TLE986x_device"/>
        <accept Dvendor="Infineon:7" Dname="TLE986*20"/>
    </condition>
    <!-- Combination of SDADC condition and fSYS condition -->
    <condition id="cond_TLE986x_noSDADC_40MHz">
      <description>Infineon TLE986x device series, no SDADC, fSYS 40MHz</description>
      <require condition="cond_TLE986x_noSDADC"/>
      <require condition="cond_TLE986x_40MHz"/>
    </condition>
    <condition id="cond_TLE986x_noSDADC_24MHz">
      <description>Infineon TLE986x device series, no SDADC, fSYS 24MHz</description>
