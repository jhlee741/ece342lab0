					<algorithm  name="Flash/SC32F53xx_BOOT.FLM"        start="0x00000000"  size="0x10000"  RAMstart="0x20000000"  RAMsize="0x0800"  default="1"/>
					<debug svd="SVD/SC32F5364.svd"/>
