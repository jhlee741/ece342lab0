  <conditions>
    <!-- conditions selecting Compiler -->
    <condition id="Compiler ARM">
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="FT32F1xx">
      <description>FremontMicro FT32F1xx Device</description>
      <require Dvendor="FMD:169" Dname="FT32F1*"/>
    </condition>
    <condition id="FT32F1xx MD">
      <description>FremontMicro FT32F1xx Mid Density Device</description>
      <require Dvendor="FMD:169" Dname="FT32F1*3?[8B]"/>
    </condition>
    <condition id="FT32F1xx HD">
      <description>FremontMicro FT32F1xx High Density Device</description>
      <require Dvendor="FMD:169" Dname="FT32F103?[CDE]"/>
    </condition>

    <condition id="FT32F1xx MD ARMCC">
      <description>filter for FT32F1xx Mid Density Device and the ARM compiler</description>
      <require condition="FT32F1xx MD"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="FT32F1xx HD ARMCC">
      <description>filter for FT32F1xx High Density Device and the ARM compiler</description>
      <require condition="FT32F1xx HD"/>
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="FT32F1xx CMSIS">
      <description>FremontMicro FT32F1xx Device with CMSIS</description>
      <require condition="FT32F1xx"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
    </condition>

    <condition id="FT32F1xx CMSIS GPIO">
      <description>FremontMicro FT32F1xx Device with CMSIS and GPIO</description>
      <require condition="FT32F1xx"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Cclass="Device" Cgroup="GPIO" />
    </condition>
   
    <condition id="FT32F1xx CMSIS GPIO DMA">
      <description>FremontMicro FT32F1xx Device with CMSIS, GPIO and DMA</description>
      <require condition="FT32F1xx"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Cclass="Device" Cgroup="GPIO" />
      <require Cclass="Device" Cgroup="DMA" />
    </condition>

    <condition id="FT32F1xx CMSIS GPIO SPI">
      <description>FremontMicro FT32F1xx Device with CMSIS, GPIO and SPI Driver</description>
      <require condition="FT32F1xx"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Cclass="CMSIS Driver" Cgroup="SPI" />
    </condition>

    <condition id="FT32F1xx CMSIS RTOS I2C">
      <description>FremontMicro FT32F1xx Device with CMSIS, RTOS and I2C Driver</description>
