  <name>STM32L1xx_DFP</name>
  <description>STMicroelectronics STM32L1 Series Device Support and Examples</description>
  
  <requirements>
    <packages>
      <package vendor="ARM" name="CMSIS" version="5.7.0"/> <!-- minimum required version for VIO driver -->
    </packages>
