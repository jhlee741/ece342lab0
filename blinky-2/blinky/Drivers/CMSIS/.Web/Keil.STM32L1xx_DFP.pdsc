      <!-- ************************  Subfamily 'STM32L100'  **************************** -->
      <subFamily DsubFamily="STM32L100">
        <processor Dfpu="0" Dmpu="1" Dendian="Little-endian" Dclock="32000000"/>
        <debug svd="CMSIS/SVD/STM32L100.svd"/>
        <debugvars configfile="CMSIS/Debug/STM32L100_151_152_162.dbgconf">
          __var DbgMCU_CR      = 0x00000007;   // DBGMCU_CR: DBG_SLEEP, DBG_STOP, DBG_STANDBY
          __var DbgMCU_APB1_Fz = 0x00000000;   // DGBMCU_APB1_FZ1: All Peripherals Operate as in Normal Mode
