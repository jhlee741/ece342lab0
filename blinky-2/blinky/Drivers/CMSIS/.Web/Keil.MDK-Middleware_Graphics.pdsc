  <name>MDK-Middleware_Graphics</name>
  <description>Middleware Graphics for Keil MDK-Professional and MDK-Plus</description>
  <url>https://www.keil.com/pack/</url>
  <license>license.rtf</license>
  <releases>
    <release version="1.3.0" date="2023-07-03">
      Graphics Component Segger emWin:
        - version 6.32.3 (see revision history for details).
        - GUI libraries build with GUI_USE_ARGB=1
    </release>
    <release version="1.2.0" date="2023-01-26">
      Graphics Component Segger emWin:
        - version 6.30.1 (see revision history for details).
    </release>
    <release version="1.1.0" date="2022-04-26">
      Added support for Arm Cortex-M85 processor based devices.
      Added support for Arm China Star-MC1 processor based devices.
      Graphics Component Segger emWin:
        - Version 6.24.0  (see revision history for details).
        - Libraries built with Arm Compiler 6.
      Updated VNC Server (new file GUI_VNC_X_StartServerRTE.c).
      Deleted obsolete file GUI_X_CMSIS_RTOS.c (replaced by GUI_X_RTE.c).
      Removed AppWizardTrial_#_#_Install.exe from Tools folder.
    </release>
    <release version="1.0.1" date="2022-01-03">
      Added AppWizard support.
    </release>
    <release version="1.0.0" date="2021-12-10">
      Initial Release.
      Graphics Component Segger emWin:
        - version 6.20.2  (see revision history for details).
    </release>
  </releases>

  <requirements>
    <packages>
      <package vendor="ARM"  name="CMSIS"        version="5.9.0-0"/>
      <package vendor="Keil" name="ARM_Compiler" version="1.7.2-0"/>
    </packages>
  </requirements>

  <apis>
    <api Cclass="Board Support" Cgroup="emWin LCD" Capiversion="1.1.0" exclusive="0">
      <description>emWin LCD Interface</description>
      <files>
        <file category="header" name="emWin/Include/LCD_X.h"/>
      </files>
    </api>
  </apis>

  <conditions>
    <condition id="Cortex-M Device">
      <description>Cortex-M processor based device: Cortex-M0/M0+/M1/M3/M4/M7/M23/M33/M35P/M55/M85, ARMV8MBL/ML, Star-MC1, SC000/300</description>
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <accept Dcore="Cortex-M1"/>
      <accept Dcore="Cortex-M3"/>
      <accept Dcore="Cortex-M4"/>
      <accept Dcore="Cortex-M7"/>
      <accept Dcore="Cortex-M23"/>
      <accept Dcore="Cortex-M33"/>
      <accept Dcore="Cortex-M35P"/>
      <accept Dcore="Cortex-M55"/>
      <accept Dcore="Cortex-M85"/>
      <accept Dcore="ARMV8MBL"/>
      <accept Dcore="ARMV8MML"/>
      <accept Dcore="Star-MC1"/>
      <accept Dcore="SC000"/>
      <accept Dcore="SC300"/>
    </condition>
    <condition id="CMSIS Core">
      <description>CMSIS Core for Cortex-M</description>
      <require Tcompiler="ARMCC"/>
      <require condition="Cortex-M Device"/>
      <require Cclass="CMSIS" Cgroup="CORE" Cversion="5.0.1"/>
    </condition>
    <condition id="CMSIS RTOS">
      <description>CMSIS RTOS (API1 or API2)</description>
      <accept Cclass="CMSIS" Cgroup="RTOS"/>
      <accept Cclass="CMSIS" Cgroup="RTOS2"/>
    </condition>
    <condition id="CMSIS RTOS1">
      <description>CMSIS RTOS (API1 only)</description>
      <require Cclass="CMSIS" Cgroup="RTOS"/>
    </condition>
    <condition id="CMSIS RTOS2">
      <description>CMSIS RTOS (API2 only)</description>
      <require Cclass="CMSIS" Cgroup="RTOS2"/>
    </condition>
    <condition id="CMSIS Core with RTOS and Display">
      <description>CMSIS Core with RTOS for Cortex-M and Display Interface</description>
      <require condition="CMSIS Core"/>
      <require condition="CMSIS RTOS"/>
      <accept  Cclass="Graphics" Cgroup="Display"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD"/>
    </condition>
    <condition id="Graphics Core">
      <description>Graphics CORE component</description>
      <require condition="CMSIS Core"/>
      <require Cclass="Graphics" Cgroup="CORE"/>
    </condition>
    <condition id="Graphics Core and Joystick">
      <description>Graphics CORE component and Joystick driver</description>
      <require condition="Graphics Core"/>
      <require Cclass="Board Support" Cgroup="Joystick"/>
    </condition>
    <condition id="Graphics Core and Touchscreen">
      <description>Graphics CORE component and Touchscreen driver</description>
      <require condition="Graphics Core"/>
      <require Cclass="Board Support" Cgroup="Touchscreen"/>
    </condition>
    <condition id="Graphics Core and LCD 16-bit IF">
      <description>Graphics CORE component and LCD driver (16-bit Interface)</description>
      <require condition="Graphics Core"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD" Csub="16-bit IF"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD" Cvariant="16-bit IF"/>
    </condition>
    <condition id="Graphics Core and LCD SPI IF">
      <description>Graphics CORE component and LCD driver (SPI Interface)</description>
      <require condition="Graphics Core"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD" Csub="SPI IF"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD" Cvariant="SPI IF"/>
    </condition>
    <condition id="Graphics Core and LCD RGB IF">
      <description>Graphics CORE component and LCD driver (RGB Interface)</description>
      <require condition="Graphics Core"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD" Csub="RGB IF"/>
      <accept  Cclass="Board Support" Cgroup="emWin LCD" Cvariant="RGB IF"/>
    </condition>
    <condition id="Graphics Core with CMSIS RTOS and BSD Socket">
      <description>Graphics CORE component with CMSIS RTOS and BSD Socket Interface</description>
      <require condition="Graphics Core"/>
      <accept  condition="CMSIS RTOS"/>
      <accept  condition="CMSIS RTOS2"/>
      <require Cclass="Network" Cgroup="Socket" Csub="BSD"/>
    </condition>
    <condition id="Graphics Demo">
      <description>Graphics Demo Framework</description>
      <require condition="CMSIS Core"/>
      <require Cclass="Graphics" Cgroup="Demo" Csub="Framework"/>
    </condition>
    <!-- ARMCC -->
    <condition id="CM0_LE_ARMCC">
      <description>Cortex-M0 or Cortex-M0+ or SC000 processor based device in little endian mode for the ARM Compiler</description>
      <accept Dcore="Cortex-M0"/>
      <accept Dcore="Cortex-M0+"/>
      <accept Dcore="SC000"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM1_LE_ARMCC">
      <description>Cortex-M1 based device in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M1"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM3_LE_ARMCC">
      <description>Cortex-M3 or SC300 processor based device in little endian mode for the ARM Compiler</description>
      <accept Dcore="Cortex-M3"/>
      <accept Dcore="SC300"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM4_LE_ARMCC">
      <description>Cortex-M4 processor based device in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M4" Dfpu="NO_FPU" Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM4F_LE_ARMCC">
      <description>Cortex-M4 processor based device using Floating Point Unit in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M4" Dfpu="FPU" Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM7_LE_ARMCC">
      <description>Cortex-M7 processor based device in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M7" Dfpu="NO_FPU" Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM7F_LE_ARMCC">
      <description>Cortex-M7 processor based device using Floating Point Unit in little endian mode for the ARM Compiler</description>
      <accept Dcore="Cortex-M7" Dfpu="SP_FPU" Dendian="Little-endian"/>
      <accept Dcore="Cortex-M7" Dfpu="DP_FPU" Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM23_LE_ARMCC">
      <description>Cortex-M23 or ARMv8-M Baseline processor based device in little endian mode for the ARM Compiler</description>
      <accept Dcore="Cortex-M23"/>
      <accept Dcore="ARMV8MBL"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM33_LE_ARMCC">
      <description>Cortex-M33 or ARMv8-M Mainline processor based device in little endian mode for the ARM Compiler</description>
      <accept Dcore="Cortex-M33" Dfpu="NO_FPU"/>
      <accept Dcore="ARMV8MML"   Dfpu="NO_FPU"/>
      <accept Dcore="Star-MC1"   Dfpu="NO_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM33F_LE_ARMCC">
      <description>Cortex-M33 or ARMv8-M Mainline processor based device using Floating Point Unit in little endian mode for the ARM Compiler</description>
      <accept Dcore="Cortex-M33" Dfpu="SP_FPU"/>
      <accept Dcore="ARMV8MML"   Dfpu="SP_FPU"/>
      <accept Dcore="ARMV8MML"   Dfpu="DP_FPU"/>
      <accept Dcore="Star-MC1"   Dfpu="SP_FPU"/>
      <accept Dcore="Star-MC1"   Dfpu="DP_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM35P_LE_ARMCC">
      <description>Cortex-M35P processor based device in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M35P" Dfpu="NO_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM35PF_LE_ARMCC">
      <description>Cortex-M35P processor based device using Floating Point Unit in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M35P" Dfpu="SP_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM55_LE_ARMCC">
      <description>Cortex-M55 processor based device in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M55" Dfpu="NO_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM55F_LE_ARMCC">
      <description>Cortex-M55 processor based device using Floating Point Unit in little endian mode for the ARM Compiler</description>
      <accept  Dcore="Cortex-M55" Dfpu="SP_FPU"/>
      <accept  Dcore="Cortex-M55" Dfpu="DP_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM85_LE_ARMCC">
      <description>Cortex-M85 processor based device in little endian mode for the ARM Compiler</description>
      <require Dcore="Cortex-M85" Dfpu="NO_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM85F_LE_ARMCC">
      <description>Cortex-M85 processor based device using Floating Point Unit in little endian mode for the ARM Compiler</description>
      <accept  Dcore="Cortex-M85" Dfpu="SP_FPU"/>
      <accept  Dcore="Cortex-M85" Dfpu="DP_FPU"/>
      <require Dendian="Little-endian"/>
      <require Tcompiler="ARMCC"/>
    </condition>
  </conditions>

