        <algorithm name="devices/LPC54608/arm/LPC5460x_MT25QL128.FLM" start="0x10000000" size="0x08000000" RAMstart="0x20000000" RAMsize="0x00002000" default="1"/>
        <debug svd="devices/LPC54608/LPC54608.xml"/>
        <variant Dvariant="LPC54608J512BD208">
