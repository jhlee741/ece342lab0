              BS_Setup     = Read32(0x0080C004);
              BS_Length    = (BS_Setup &amp; 0x000FFF00) &gt;&gt;  8;
              BS_Length   *= 0x100;
              BS_HashAddr  = BS_Length - BNSC_Length - 0x20;
