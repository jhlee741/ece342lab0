              __var FP_CTRL_Addr       = 0xE0002000;
              __var FP_COMP0_Addr      = 0xE0002008;
              __var FPB_KEY            = 0x00000002;
              __var DWT_COMP0_Addr     = 0xE0001020;
              __var DWT_FUNCTION0_Addr = 0xE0001028;
