      <!-- MAX32672/51/52 CMSIS-CORE Condition -->
      <condition id="MAX32672 CMSIS-CORE">
         <description>Maxim MAX32672 Series devices and CMSIS-CORE</description>
         <require Cclass="CMSIS" Cgroup="CORE"/>
         <require condition="MAX32672 Family"/>
     <!--    <accept condition="MAX32676 Device"/> -->
		 <accept condition="MAX32672 Device"/>
         <require Cclass="Device" Cgroup="Startup"/>
      </condition>

      <!-- GNUC/Eclipse Compiler Condition -->
