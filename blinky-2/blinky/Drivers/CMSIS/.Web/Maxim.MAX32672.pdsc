      </condition>

      <!-- MAX32672 Device Condition -->
      <condition id="MAX32672 Device">
         <description> Maxim Integrated MAX32672 device family</description>
         <require Dvendor="Maxim:23"/>
         <require Dname="MAX32672"/>
      </condition>


      <!-- MAX32676 Device Condition -->
