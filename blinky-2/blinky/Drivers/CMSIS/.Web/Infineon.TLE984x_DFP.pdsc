  <conditions>
    <!-- Conditions on devices -->
    <condition id="cond_TLE984x_all">
      <description>Infineon TLE984x device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE984*QX"/>
    </condition>
    <condition id="cond_TLE9842QX">
      <description>Infineon TLE9842QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9842QX"/>
    </condition>
    <condition id="cond_TLE9842-2QX">
      <description>Infineon TLE9842-2QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9842-2QX"/>
    </condition>
    <condition id="cond_TLE9843QX">
      <description>Infineon TLE9843QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9843QX"/>
    </condition>
    <condition id="cond_TLE9843-2QX">
      <description>Infineon TLE9843-2QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9843-2QX"/>
    </condition>
    <condition id="cond_TLE9844QX">
      <description>Infineon TLE9844QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9844QX"/>
    </condition>
    <condition id="cond_TLE9844-2QX">
      <description>Infineon TLE9844-2QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9844-2QX"/>
    </condition>
    <condition id="cond_TLE9845QX">
      <description>Infineon TLE9845QX device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Dvendor="Infineon:7" Dname="TLE9845QX"/>
    </condition>
    <!-- Conditions for components -->
    <condition id="cond_TLE984x_ADC1">
      <accept condition="cond_TLE984x_all"/>
      <!-- ADC1 requires ADC2 -->
      <require Cclass="Device" Cgroup="SDK" Csub="ADC2"/>
    </condition>
    <!-- Conditions for ConfigWizard -->
    <condition id="cond_ConfigWizard1">
      <description>If ConfigWizard v1 is selected, version 2.x must not be selected</description>
      <deny Cclass="Device" Cgroup="ConfigWizard_v2" Csub=""/>
      <require condition="cond_TLE984x_all"/>
    </condition>
    <condition id="cond_ConfigWizard2">
      <description>If ConfigWizard v2.x is selected, version 1.x must not be selected</description>
      <deny Cclass="Device" Cgroup="ConfigWizard" Csub=""/>
      <require condition="cond_TLE984x_all"/>
    </condition>
    <!-- Conditions for startup -->
    <condition id="cond_TLE984x_startup">
      <description>Infineon TLE984x device series</description>
      <require Cclass="CMSIS" Cgroup="CORE" Csub=""/>
      <require Cclass="Device" Cgroup="SDK" Csub="SCU"/>
      <require Cclass="Device" Cgroup="SDK" Csub="BOOTROM"/>
      <require Cclass="Device" Cgroup="SDK" Csub="WDT1"/>
      <require Cclass="Device" Cgroup="SDK" Csub="ISR"/>
      <require Cclass="Device" Cgroup="SDK" Csub="INT"/>
      <require Cclass="Device" Cgroup="SDK" Csub="PORT"/>
      <require Cclass="Device" Cgroup="SDK" Csub="TRX"/>
      <require Cclass="Device" Cgroup="SDK" Csub="PMU"/>
      <require condition="cond_TLE984x_all"/>
      <accept condition="cond_ConfigWizard1"/>
      <accept condition="cond_ConfigWizard2"/>
    </condition>
    <!-- Conditions for compilers -->
    <condition id="cond_TLE984x_ARM">
      <description>Infineon TLE984x device series with ARMCC</description>
      <require Tcompiler="ARMCC"/>
      <require condition="cond_TLE984x_all"/>
    </condition>
    <condition id="cond_TLE984x_IAR">
      <description>Infineon TLE984x device series with IAR</description>
      <require Tcompiler="IAR"/>
      <require condition="cond_TLE984x_all"/>
    </condition>
  </conditions>
