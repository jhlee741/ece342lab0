      <condition id="Startup ARM">
         <description>Startup files for the KEIL Embedded Workbench Toolchain</description>
         <require Tcompiler="ARMCC"/>
      </condition> 
      <condition id="Startup IAR">
         <description>Startup files for the IAR Embedded Workbench Toolchain</description>
         <require Tcompiler="IAR"/>
      </condition>

      <!-- ADC Peripheral Condition -->
      <condition id="MAX32655 CMSIS ADC">
         <description>ADC Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>
	  
      <!-- TFT Peripheral Condition -->
      <condition id="MAX32655 CMSIS TFT">
         <description>TFT Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>	  

      <!-- AES Peripheral Condition -->
      <condition id="MAX32655 CMSIS AES">
         <description>AES Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>
	  
      <!-- CRC Peripheral Condition -->
      <condition id="MAX32655 CMSIS CRC">
         <description>CRC</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>	
	
      <!-- Standard DMA Peripheral -->
      <condition id="MAX32655 CMSIS DMA">
         <description>Standard DMA</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>	  

