      <!-- GPIO -->
      <condition id="MAX32655 CMSIS GPIO">
         <description>GPIO</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- I2C -->
      <condition id="MAX32655 CMSIS I2C">
         <description>I2C Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- I2S -->
      <condition id="MAX32655 CMSIS I2S">
         <description>I2S Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- ICC -->
      <condition id="MAX32655 CMSIS ICC">
         <description>Instruction Cache Controller</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- Low Power -->
      <condition id="MAX32655 CMSIS Low Power">
         <description>Low Power API</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- RTC -->
      <condition id="MAX32655 CMSIS RTC">
         <description>Real-Time Clock</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- SPI Peripheral -->
      <condition id="MAX32655 CMSIS SPI">
         <description>SPI Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- SYS -->
      <condition id="MAX32655 CMSIS SYS">
         <description>SYS Interface</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- Timer -->
      <condition id="MAX32655 CMSIS TIMER">
         <description>Timers</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- TRNG -->
      <condition id="MAX32655 CMSIS TRNG">
         <description>Timers</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- UART -->
      <condition id="MAX32655 CMSIS UART">
         <description>UART</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>

      <!-- WDT -->
      <condition id="MAX32655 CMSIS WDT">
         <description>Watchdog Timers</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>
	  
      <!-- WUT -->
      <condition id="MAX32655 CMSIS WUT">
         <description>Wakeup Timer</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>	 

      <!-- PT -->
      <condition id="MAX32655 CMSIS PT">
         <description>Pulse Train</description>
         <require condition="MAX32655 CMSIS-CORE"/>
      </condition>	  

