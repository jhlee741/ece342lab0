      <!-- MAX32655/51/52 CMSIS-CORE Condition -->
      <condition id="MAX32655 CMSIS-CORE">
         <description>Maxim MAX32655 Series devices and CMSIS-CORE</description>
         <require Cclass="CMSIS" Cgroup="CORE"/>
         <require condition="MAX32655 Family"/>
