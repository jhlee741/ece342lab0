      <!-- MAX32655 Device Condition -->
      <condition id="MAX32655 Family">
         <description> Maxim Integrated MAX32655 device family</description>
         <require Dvendor="Maxim:23"/>
		 <require Dname="MAX32655"/>
      </condition>

      <!-- MAX32655 Device Condition -->
      <condition id="MAX32655 Device">
         <description> Maxim Integrated MAX32655 device family</description>
         <require Dvendor="Maxim:23"/>
         <require Dname="MAX32655"/>
      </condition>

      <!-- MAX32656 Device Condition -->
