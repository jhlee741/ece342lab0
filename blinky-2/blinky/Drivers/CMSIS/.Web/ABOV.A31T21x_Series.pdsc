  <devices>
    <family Dfamily="A31T21x Series" Dvendor="ABOV Semiconductor:126">
      <processor Dcore="Cortex-M0+" DcoreVersion="r0p0" Dfpu="0" Dmpu="0" Dendian="Little-endian" />
      <description>The ABOV A31T21x Series device family contains an ARM Cortex-M0+ processor with a versatile set of on-chip peripherals</description>
      <!-- Subfamily : A31T21x *********************************************************-->
      <subFamily DsubFamily="A31T21x">
        <processor Dclock="48000000" />
        <compile header="A31T21x/core/device/a31t21x/include/a31t21x.h" />
        <debug svd="A31T21x/toolchain/keil/svd/a31t21x.svd" />
          <feature type="VCC" n="1.8" m="5.5" />
	      <feature type="NVIC"            n="32"                    name="NVIC with 32 Interrupt Sources" />
	      <feature type="DMA"             n="4"                     name="DMA" />
	      <feature type="Memory"          n="256"                   name="Internal Code Flash Memory (CFMC)" />
	      <feature type="XTAL"            n="4000000" m="16000000"  name="External Crystal Oscillator (XTAL) "/>
	      <feature type="IntRC"           n="32000000"              name="Internal RC Oscillator with +/- 1% accuracy"/>
          <feature type="PLL"             n="1"                     name="Internal PLL up to 48MHz"/>
          <feature type="PowerMode"       n="3"                     name="Run, Sleep (Idle), Deep-Sleep (Power-down)"/>
          <feature type="PowerOther"      n="1"                     name="POR"/>
          <feature type="IOs"             n="59"                    name="GPIOs of 64-LQFP Package"/>
          <feature type="IOs"             n="44"                    name="GPIOs of 48-LQFP Package"/>
          <feature type="IOs"             n="40"                    name="GPIOs of 44-LQFP Package"/>
          <feature type="IOs"             n="28"                    name="GPIOs of 32-LQFP Package"/>
	      <feature type="UART"            n="2"                     name="UART Interface" />
	      <feature type="USART"           n="2"       				name="USART Interface" />
	      <feature type="SPI"             n="2"                     name="SPI Interface" />
	      <feature type="I2C"             n="2"                     name="I2C Interface" />
	      <feature type="ADC"             n="150"     m="12"        name="150 ksps High Performance 12-bit ADC" />
	      <feature type="Timer"           n="4"       m="16"        name="16-bit TIMER (TIMER1n)" />
	      <feature type="Timer"           n="2"       m="32"        name="32-bit TIMER (TIMER2n)" />
	      <feature type="PWM"             n="1"       m="16"        name="3-phase complementary PWM (TIMER3n)" />
	      <feature type="WDT"             n="1"                     name="Watch-dog Timer" />
	      <feature type="LCD"             n="1"       m="8.32"      name="LCD Driver (8-COM x 32-SEG)" />
	      <feature type="Touch"           n="1"       m="24"		name="Touch Key (24-ch)" />
		  <feature type="QFP"             n="64"                    name="64-LQFP Package (10x10x1.6mm)"/>
		  <feature type="QFP"             n="48"                    name="48-LQFP Package (7x7x1.6mm)"/>
		  <feature type="QFP"             n="44"                    name="44-LQFP Package (10x10x1.6mm)"/>
		  <feature type="QFP"             n="32"                    name="32-LQFP Package (7x7x1.6mm)"/>


        <memory id="IRAM1" start="0x20000000" size="0x4000" init="0" default="1" />
        <!-- Device : A31T214-->
        <device Dname="A31T214">
          <description>128KB Flash Memory</description>
          <algorithm name="A31T21x\toolchain\keil\flashloader\a31t21x_flash.flm" start="0x00000000" size="0x20000" default="1" />
          <memory id="IROM1" start="0x00000000" size="0x20000" startup="1" default="1" />
        </device>
        <!-- Device : A31T216-->
        <device Dname="A31T216">
          <description>256KB Flash Memory</description>
          <algorithm name="A31T21x\toolchain\keil\flashloader\a31t21x_flash.flm" start="0x00000000" size="0x40000" default="1" />
          <memory id="IROM1" start="0x00000000" size="0x40000" startup="1" default="1" />
        </device>
      </subFamily>
    </family>
  </devices>
