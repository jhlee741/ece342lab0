      <require Cclass="CMSIS" Cgroup="CORE" />
      <accept Dvendor="ABOV Semiconductor:126" Dname="A31T214" />
      <accept Dvendor="ABOV Semiconductor:126" Dname="A31T216" />
    </condition>
  </conditions>

