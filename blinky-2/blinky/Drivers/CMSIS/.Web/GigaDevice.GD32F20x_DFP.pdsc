  <conditions>
    <!-- Compiler Conditions -->
    <condition id="Compiler ARMCC">
      <require Tcompiler="ARMCC"/>
    </condition>

    <!-- Device Conditions -->
    <condition id="GD32F20xCL">
      <description>GigaDevice GD32F20x Devices</description>
      <require Dvendor="GigaDevice:123" Dname="GD32F20[57]*"/>
      <require Cclass="Device" Cgroup="Startup" />
    </condition>

   <!-- Device + CMSIS Conditions -->
    <condition id="GD32F20x CMSIS">
      <description>GigaDevice GD32F20x Devices and CMSIS-CORE</description>
      <accept condition="GD32F20xCL"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Cclass="Device" Cgroup="GD32F20x_libopt"/>    
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="RCU"/>           
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="PMU"/>    
    </condition>
    <!-- GD32F20x STDPERIPHERALS RCU Conditions -->
    <condition id="GD32F20x STDPERIPHERALS RCU">
      <description>GigaDevice  GD32F20x Standard Peripherals Drivers with RCU</description>
      <require condition="GD32F20x CMSIS"/>
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="RCU"/>
    </condition>

     <!-- GD32F20x STDPERIPHERALS MISC Conditions -->
    <condition id="GD32F20x STDPERIPHERALS MISC">
      <description>GigaDevice  GD32F20x Standard Peripherals Drivers with MISC</description>
      <require condition="GD32F20x CMSIS"/>
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="MISC"/>
    </condition>

     <!-- GD32F20x STDPERIPHERALS EVAL Conditions -->
    <condition id="GD32F20x STDPERIPHERALS EVAL">
      <description>GigaDevice  GD32F20x Standard Peripherals Drivers with EVAL</description>
      <require condition="GD32F20x CMSIS"/>
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="GPIO"/>
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="EXTI"/>
      <require Cclass="Device" Cgroup="GD32F20x_StdPeripherals" Csub="USART"/>
    </condition>
  </conditions>

