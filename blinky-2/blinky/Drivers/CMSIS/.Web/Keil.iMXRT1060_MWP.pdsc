      <package vendor="NXP"  name="MIMXRT1060-EVKC_BSP" version="17.0.0"/>
    </packages>
  </requirements>

  <keywords>
    <!-- keywords for indexing -->
    <keyword>NXP</keyword>
    <keyword>iMXRT</keyword>
    <keyword>iMXRT1060</keyword>
    <keyword>iMXRT1061</keyword>
    <keyword>iMXRT1062</keyword>
