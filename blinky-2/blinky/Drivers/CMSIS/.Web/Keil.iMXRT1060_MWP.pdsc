  <conditions>
    <condition id="MIMXRT1060">
      <description>NXP i.MX RT 1060 device</description>
      <accept Dvendor="NXP:11" Dname="MIMXRT1061*"/>
      <accept Dvendor="NXP:11" Dname="MIMXRT1062*"/>
    </condition>

    <condition id="MIMXRT1060 CMSIS">
      <description>NXP i.MX RT 1060 device and CMSIS-CORE</description>
      <require condition="MIMXRT1060"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
    </condition>

    <condition id="MIMXRT1060 CMSIS SDK_CAN">
      <description>NXP i.MX RT 1060 device, CMSIS-CORE and SDK drivers for CMSIS CAN driver</description>
      <require condition="MIMXRT1060 CMSIS"/>
      <require Cclass="Device" Cgroup="SDK Drivers" Csub="flexcan"/>
    </condition>

    <condition id="MIMXRT1060 CMSIS EMAC">
      <description>NXP i.MX RT 1060 device, CMSIS-CORE and SDK drivers for CMSIS EMAC driver</description>
      <require condition="MIMXRT1060 CMSIS"/>
      <require Cclass="Device" Cgroup="SDK Drivers" Csub="enet"/>
    </condition>

    <condition id="MIMXRT1060 CMSIS MCI">
      <description>NXP i.MX RT 1060 device, CMSIS-CORE and SDK drivers for CMSIS MCI driver</description>
      <require condition="MIMXRT1060 CMSIS"/>
      <require Cclass="Device" Cgroup="SDK Drivers" Csub="sdhc"/>
      <require Cclass="Device" Cgroup="SDK Drivers" Csub="gpio"/>
    </condition>
  </conditions>

